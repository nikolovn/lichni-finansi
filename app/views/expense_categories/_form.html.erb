<%= form_for(@expense_category) do |c| %>
  <div class="field">
    <%= c.text_field :name %>
    <%= c.select('parent_id',  options_from_collection_for_select(@expense_parent_categories , 'id', 'name'), {include_blank: 'None'})
%>
  </div>
  <div class="actions">
    <%= c.submit  "#{t 'income_categories.new.save'}"%>
  </div>
<% end %>
