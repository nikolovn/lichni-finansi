<%= form_for(@income_transaction) do |f| %>
 <div class="form-group">
      <%= f.hidden_field :income_category_id, :value => income_category_id, :id => 'income_category_id'  %>
      <%= f.label "#{t 'expense_transactions.description'}" %><br>
      <%= f.text_area :description, class: "form-control" %><br>
      <%= f.label "#{t 'expense_transactions.amount'}" %><br>
      <%= f.text_field :amount, class: "form-control" %>
      <%= f.label "#{t 'expense_transactions.date'}" %><br>
      <%= f.text_field :date, class: "form-control" %>
  </div>
 <%= f.submit %>
<% end %>
