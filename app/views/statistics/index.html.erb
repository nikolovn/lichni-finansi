                  <% params = {q: { date_gteq: hash[:date], date_lteq: hash[:date], tabs: 'expense'}} %>
                  <% params = params.merge(params[:q])%>
                  <%=params[:q]%>
                <td><%= link_to("#{hash[:date]}", all_transactions_path(params))  %></td>
